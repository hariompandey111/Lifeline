# ===========================================
# LTHS Environment Variables
# ===========================================
# Copy this file to .env and fill in your values
# Nuxt 3 automatically maps NUXT_* env vars to runtimeConfig

# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
NUXT_SUPABASE_URL=https://your-project.supabase.co
NUXT_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Resend Email Configuration  
# Get your API key from: https://resend.com/api-keys
NUXT_RESEND_API_KEY=re_your_api_key

# Email Settings (optional - has defaults)
# EMAIL_FROM requires a verified domain in Resend, or use the default onboarding@resend.dev
NUXT_EMAIL_FROM=LTHS Contact <onboarding@resend.dev>
NUXT_EMAIL_TO=aman.l@lths.in
